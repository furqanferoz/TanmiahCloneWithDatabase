
@*@model TanmiahClone.Models.BreadCrumModel*@

@if (Model != null)
{

    <div class="breadcrumbs">
        <div class="container">
            <div class="row">
                <div class="col">
                    <ul class="breadcrumbs-list">
                        <li>
                            <a href="javascript:void(0)">@Model.MainLink</a>
                        </li>
                        <li>
                            <a href="javascript:void(0)">@Model.SubLink</a>
                        </li>
                        <li>
                            <a href="javascript:void(0)">@Model.UnderSubLink</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
}